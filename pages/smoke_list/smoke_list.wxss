/* pages/deep_cleaning/deep_cleaning.wxss */
.deep {
  position: absolute;
  width: 100%;
  height: 100%;
  background: #f6f7f9;
  padding: 30rpx;
  box-sizing:border-box;
}
.deep_list {
  width: 100%;
}
.deep_list_item {
  display: flex;
  margin-bottom: 20rpx;
  background: #fff;
  border:1rpx solid #efefef;
}
.right {
  flex: 1;
  margin-left: 20rpx;
}
.left image {
  width: 150rpx;
  height: 150rpx;
}
.area {
  color: #999;
  font-size: 28rpx;
  padding: 10rpx 0;
}
.name {
  color: #333;
  font-size: 32rpx;
  margin-top: 20rpx;
}
.price {
  color: #f44;
  font-size: 28rpx;
  margin-top:20rpx;
}
.meter_title {
  font-size: 32rpx;
  padding-bottom: 30rpx;
}
.notice {
  font-size: 26rpx;
  color: #7f8085;
  padding-top: 30rpx;
  line-height: 48rpx;
}
.btn {
  width: 100%;
  position: fixed;
  box-sizing: border-box;
  background: #fff;
  left: 0;
  bottom: 0;
  padding: 20rpx;
  border-top: 1rpx solid #efefef;
}
.btn_an {
  width: 100%;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  font-size: 28rpx;
  color: #fff;
  background: #76aef8;
}


.arcitve {
  border-color: #f44;
}